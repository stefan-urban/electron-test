{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist-linux64": "electron-builder --publish never --linux --x64",
    "dist-win64": "electron-builder --publish never --win --x64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": {
	"name": "Test Person",
	"email": "test@example.org",
	"url": "http://example.org"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "org.example.electronquickstart",
    "productName": "ElectronQuickstart",
    "win": {
      "target": ["zip"]
    },
    "linux": {
      "target": ["deb", "tar.gz"]
    }
  }
}
